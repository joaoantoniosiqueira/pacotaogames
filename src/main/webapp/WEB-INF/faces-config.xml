<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <navigation-rule>
  <from-view-id>/game/listar.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>gotoCarrinho</from-outcome>
   <to-view-id>/venda/carrinho.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/venda/carrinho.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>gotoCheckout</from-outcome>
   <to-view-id>/venda/checkout.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/home/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>refresh</from-outcome>
   <to-view-id>/home/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>gotoIndex</from-outcome>
   <to-view-id>/home/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
